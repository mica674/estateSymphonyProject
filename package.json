{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "db_reset": "npx sequelize-cli db:drop --env production && npx sequelize-cli db:create --env production",
    "migrate_undo": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all --env production",
    "migrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate --env production",
    "seed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:all --env production",
    "dev": "nodemon index.js",
    "rebuild": "npm rebuild bcryptjs",
    "instance": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
  "engines": {
    "node": "18.x"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "google-auth-library": "^9.4.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.8",
    "nodemon": "^3.0.2",
    "pg": "^8.11.3",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "description": "",
  "devDependencies": {
    "sequelize-cli": "^6.6.2"
  }
}